  const { contract } = t.context.accounts;
  const message: string = await contract.get_greeting({});
  t.is(message, 'Hello');
});

test('changes the message', async (t) => {
  const { contract } = t.context.accounts;
  await contract.set_greeting({args:{ message: 'Howdy' }});
  const message: string = await contract.get_greeting({});
  t.is(message, 'Howdy');
});",
]
